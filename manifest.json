{
  "name": "Hello Extensions",
  "description": "Base Level Extension",
  "version": "0.1.0",
  "manifest_version": 3,
  "browser_action": {
    "default_popup": "hello.html",
    "default_icon": {
      "16": "images/logo-2.png",
      "24": "images/logo-2.png",
      "32": "images/logo-2.png"
    }
  },
  "commands": {
    "_execute_browser_action": {
      "suggested_key": {
        "default": "Ctrl+Shift+F",
        "mac": "MacCtrl+Shift+F"
      },
      "description": "Opens hello.html"
    }
  }
}
